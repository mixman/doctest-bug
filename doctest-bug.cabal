cabal-version: 2.2
-- Initial package description 'doctest-bug.cabal' generated by 'cabal
-- init'.  For further documentation, see
-- http://haskell.org/cabal/users-guide/

name:                doctest-bug
version:             0.1.0.0
-- synopsis:
-- description:
-- bug-reports:
-- license:
license-file:        LICENSE
author:              Jussi Vaihia
maintainer:          jussiava@gmail.com
-- copyright:
-- category:
build-type:          Simple
extra-source-files:  CHANGELOG.md

library
  default-language:   Haskell2010
  hs-source-dirs:     src
  ghc-options:        -Wall
  default-extensions:
    DeriveDataTypeable
    DeriveFoldable
    DeriveFunctor
    DeriveGeneric
    DeriveTraversable
    ScopedTypeVariables
  build-depends:       base >=4.14 && <4.15
    , colour
    , deepseq              ^>=1.4.1.1
    , fin                  ^>=0.1
    , hashable             ^>=1.3.0.0
    , JuicyPixels          >=3.2.7 && <3.4
    , kleene
    , template-haskell
    , text
    , th-lift              ^>=0.8
    , time                 >=1.6   && <2.0
    , time-parsers         ^>=0.1.2.0
  exposed-modules:
    Case.Doc
    Case.Colour
    Case.Integrations.Common
    Case.Integrations.Serv
    Case.Integrations.Contains

executable doctest-bug
  main-is:             Main.hs
  -- other-modules:
  -- other-extensions:
  build-depends:       base >=4.14 && <4.15
  -- hs-source-dirs:
  default-language:    Haskell2010
